rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  	match /code/{code} {
    	allow read : if true;
      allow create, write, update, delete : if false;
    }
    match /member/{member} {
    	allow read, write, update, delete : if true;
    }
    match /round/{round} {
    	allow read, write, update, delete : if true;
    }
    match /{document=**} {
      allow read, write, create, update, delete: if false;
    }
  }
}